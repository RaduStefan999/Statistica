#functia pentru Poisson(lambda)(1.a)
k=20
l=52
variabila1=10
Poisson=function(Lambda)
{
x=seq(k,l,1)
y=dpois(x,Lambda)
plot(x,y,xlab='axa x',ylab='axa y',main='grafic Poisson')
return(max(y))
}
Poisson(variabila1)
#functia pentru Geometrica(p)(1.b)
#aici puneti care este intervalul de numere dorit
k=10
l=32
p=0.5
Geometrica=function(p)
{
x=seq(k,l,1)
y=dgeom(x,p)
plot(x,y,xlab='axa x',ylab='axa y',main='grafic Geometrica')
return(sum(y))
}
Geometrica(p)
#Functia care returneaza mediana, media, sd, cvartitele(2.a)
Statisticile=function(textul)
{
x=scan(textul)
v=mean(x)
v=c(v,median(x),sd(x),as.vector(quantile(x))[1:5])
return(v)
}
Statisticile('Valori.txt')
#formatul este media, mediana, deviatia standard, Q0,Q1,Q2,Q3,Q4
#Functia care elimina valorile aberante din esantion cu ajutorul mediei si deviatiei standard
#2.b
Neaberante=function(text)
{
j=0
x=scan(text)
m=mean(x)
delta=sd(x)
for(i in 1:length(x))
if(x[i]>=m-2*delta&&x[i]<=m+2*delta)
{
DeReturnat[j]=x[i]
j=j+1
}
return(DeReturnat)
}
Neaberante('Valori.txt')
#Functia ce primeste numele fisierului si afiseaza graficul cu valori neaberante
#2.c
ReprezentareGrafica=function(text)
{
y=Neaberante(text)
interval=seq(30,100,10)
hist(y,right=F,breaks=interval,freq=T)#Am pus Right=F fiindca doresc sa fie inchis in stanga!
}
ReprezentareGrafica('Valori.txt')
